@model List<Accounts>

<h1>Accounts</h1>
<table border="1">
    <thead>
        <tr>
            <th>ID</th>
            <th>Username</th>
            <th>Email</th>
            <th>Active</th>
            <th>Created</th>
        </tr>
    </thead>    
    <tbody>
        @foreach (var account in Model)
        {
            <tr>
                <td>@account.AccountId</td>
                <td>@account.Username</td>
                <td>@account.Email</td>
                <td>@account.IsActive</td>
                <td>@account.CreationDate</td>
            </tr>
        }
    </tbody>
</table>